{
    "env":
    {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals":
    {
        "angular": true,
        "module": true,
        "inject": true,
        "moment": true,
        "saveAs": true,
        "require": false
    },
    "rules":
    {
        "indent": [0, "tab"],
        "linebreak-style": [0, "unix"],
        "semi": [2, "always"]
    }
}
